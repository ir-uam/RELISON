<algorithms>
	<algorithm> <!-- Random algorithm -->
		<name>Random</name>
	</algorithm>
	<algorithm> <!-- Popularity-based algorithm -->
		<name>Popularity</name> 
	</algorithm>
	<algorithm> <!-- BM25 algorithm -->
		<name>BM25</name>
		<params>
			<param>
				<name>weighted</name>
				<type>Boolean</type>
				<values>
					<value>true</value>
				</values>
			</param>
			<param>
				<name>uSel</name> <!-- Neighborhood selection for the target user -->
				<type>LinkOrientation</type>
				<values>
					<value>UND</value>
				</values>
			</param>
			<param>
				<name>vSel</name> <!-- Neighborhood selection for the candidate user -->
				<type>LinkOrientation</type>
				<values>
					<value>IN</value>
				</values>
			</param>
			<param>
				<name>dlSel</name> <!-- Neighborhood selection for the user length -->
				<type>LinkOrientation</type>
				<values>
					<value>OUT</value>
				</values>
			</param>
			<param>
				<name>b</name> <!-- Parameter that tunes the effect of the neighborhood size. Between 0 and 1 -->
				<type>Double</type>
				<values>
					<value>0.1</value>
				</values>
			</param>
			<param>
				<name>k</name> <!-- Parameter that tunes the effect of the term frequency on the formula.-->
				<type>Double</type>
				<values>
					<value>0.1</value>
				</values>
			</param>
		</params>
	</algorithm>
